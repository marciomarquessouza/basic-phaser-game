    "build:dev": "webpack --config webpack.base.js --mode=development",
    "dev": "rimraf dist && npx yarn build:dev && ts-node src/server.ts",
    "start": "rimraf dist && npx yarn build && ts-node src/server.ts",
    "e2e": "nightwatch"
  },
  "dependencies": {
    "express": "^4.17.1",
